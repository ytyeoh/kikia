<div class="row ">
  <div class="col s12 l6 title">
    <div class="row">
      <div class="col s2">
        <i class="material-icons">insert_chart</i>
      </div>
      <div class="col s10 f18 text-left">
        <b>sdfdsf</b>
      </div>
      <div class="col s10 f14 text-left">
        sdfsdgvsd
      </div>
    </div>

    <div class="row">
      <div class="col s2">
        <i class="material-icons">insert_chart</i>
      </div>
      <div class="col s10 f18 text-left">
        <b>sdfdsf</b>
      </div>
      <div class="col s10 f14 text-left">
        sdfsdgvsd
      </div>
    </div>
    <div class="row">
      <div class="col s2">
        <i class="material-icons">insert_chart</i>
      </div>
      <div class="col s10 f18 text-left">
        <b>sdfdsf</b>
      </div>
      <div class="col s10 f14 text-left">
        sdfsdgvsd
      </div>
    </div>

  </div>
  <div class="col s12 l6">
    <div class="row">
      <%= form_tag listings_path, method: :get do %>
      <div class="input-field col s12">
        <div class="row home">
          <div class="col s6 padding-zero">
            <%= text_field_tag :query, params[:query], class: 'city home-form', placeholder: "Where you want to rent?",  autocomplete: "off" %>
          </div>
          <div class="col s6 padding-zero">
            <%= select_tag :imove_in, "<option value=false>Normal Move In</option><option value=true>Instant Move In</option>".html_safe, class: 'home-form'%>
          </div>
        </div>
      </div>
      <div class="input-field col s12">
        Price Range:
        <div id="html5"></div>
        <input type="number" class="col s4 number-input" name="lower" min="500" max="2500" step="1" id="lower">
        <div class="col s4"></div>
        <input type="number" class="col s4 number-input" name="higher" min="500" max="2500" step="1" id="higher">
      </div>
      <div class="col s12">
        <%= submit_tag 'Get your room now!', class: 'btn waves-effect waves-light' %>
      </div>
      <% end %>
    </div>
  </div>
</div>
